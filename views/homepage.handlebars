<!-- Loop over each object in the projects array -->
{{#if logged_in}}

{{#each projects as |project|}}
<div class="container">
<div class="row mb-4 project">
  <div class="col-md-3">
    <h2><a href="/project/{{project.id}}">{{project.name}}</a></h2>
    <p>{{project.description}}</p>
    <p>Posted by {{project.user.name}}</p> 
    <p>{{format_date project.date_created}}</p>
  </div>
</div>
</div>
{{/each}}
{{else}}
<div></div>
{{/if}}
